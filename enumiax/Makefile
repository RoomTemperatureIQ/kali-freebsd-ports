# $FreeBSD$

PORTNAME=       enumiax
PORTVERSION=	0.4
DISTVERSIONSUFFIX=	a
CATEGORIES=	security
MASTER_SITES=	SF
MASTER_SITE_SUBDIR=	${PORTNAME}/${PORTNAME}/${PORTNAME}-${PORTVERSION}${DISTVERSIONSUFFIX}

MAINTAINER=	zackj901@yandex.com
COMMENT=	IAX protocol username enumerator

LICENSE=	GPLv2

do-install:
	${INSTALL_PROGRAM}	${WRKSRC}/enumiax	${STAGEDIR}${PREFIX}/bin
	@${MKDIR} ${STAGEDIR}${DOCSDIR}
	${INSTALL_DATA} ${WRKSRC}/README	${STAGEDIR}${DOCSDIR}/README

.include <bsd.port.mk>
